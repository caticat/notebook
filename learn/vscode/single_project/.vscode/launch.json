	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/build/playground",
			"cwd": "${workspaceFolder}/build",
			// "preLaunchTask": "build", // 想编译直接编译就行了,没必要每次运行都编译一遍
			"setupCommands": [
				{
					"description": "gdb整齐打印(map等内容可以正常显示)",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}